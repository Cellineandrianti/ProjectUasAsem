<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectuasasem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".IbuActivity">
            <!-- Aktivitas untuk ibu -->
        </activity>

        <activity android:name=".AnakActivity">
            <!-- Aktivitas untuk anak-anak -->
        </activity>

        <activity android:name=".SettingActivity">
            <!-- Aktivitas untuk pengaturan -->
        </activity>

        <activity android:name=".ChildProfileActivity">
            <!-- Aktivitas untuk profil anak -->
        </activity>

        <!-- Tambahkan aktivitas-aktivitas lain sesuai kebutuhan -->

    </application>

</manifest>
